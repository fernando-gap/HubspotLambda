{
  "name": "desafio-devapi",
  "version": "1.0.0",
  "description": "Create HubSpot Contacts from Google Spreadsheets",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "prepare": "chmod +x ./scripts/cleanup.sh ./scripts/setup.sh",
    "test": "mocha --recursive ./test -r dotenv/config",
    "lint": "npx eslint",
    "aws:setup": "./scripts/setup.sh",
    "aws:deploy": "sam build && sam deploy --guided",
    "aws:cleanup": "./scripts/cleanup.sh",
    "hubspot:cleanup": "node -r dotenv/config ./scripts/delete-contact.js",
    "git:hook": "husky install"
  },
  "author": "Fernando A",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.267.0",
    "axios": "^1.3.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "eslint": "^8.33.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "mocha": "^10.2.0"
  }
}
